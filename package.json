{
  "name": "bit_dev_test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "node-sass": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "bit-export": "bit export frankwatching.fw_7_test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@2.0.0"
    },
    "componentsDefaultDirectory": "atoms/{name}",
    "packageManager": "yarn",
    "overrides": {
      "atoms/colors": {
        "env": {
          "compiler": "-"
        }
      },
      "atoms/fonts": {
        "env": {
          "compiler": "-"
        }
      },
      "atoms/spacing": {
        "env": {
          "compiler": "-"
        }
      },
      "atoms/globals": {
        "env": {
          "compiler": "-"
        }
      },
      "atoms/icons": {
        "env": {
          "compiler": "bit.envs/compilers/react@2.0.0"
        }
      },
      "atoms/icons/styles.scss": {
        "env": {
          "compiler": "-"
        }
      },
      "frankwatching.fw_7_test/atoms/colors": {}
    }
  }
}
